{
    "manifest_version": 3,
    "name": "Naitik",
    "version": "1.0.0",
    "description": "A basic extension to battle dark patterns",
    "action": {
        "default_popup": "index.html"
    },
    "icons": { "16": "images/logo_naitik.png",
           "48": "images/logo_naitik.png",
          "128": "images/logo_naitik.png" },
    "content_scripts" : [
        {
            "matches": [
            "https://*/*",
            "http://*/*"
            ],
            "js": ["scripts/checkbox.js", "scripts/clickjacking.js"]
        }
    ],
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "scripting",
        "alarms",
        "aiLanguageModelOriginTrial"
    ],

    "key": "",
  "trial_tokens": [
    "Aozzz6KfHYqh8q5x+Khse27nSp8YM7Tftv6XZhNO7lgYcP5uQxxBEpMfRhiFbYJV+yJl1fDNzvtao7FswtZGIgQAAAB4eyJvcmlnaW4iOiJjaHJvbWUtZXh0ZW5zaW9uOi8vYWhpaWZrb2RnbWlmcGNnbmRja3BwaW1lY25wa3BkbGwiLCJmZWF0dXJlIjoiQUlQcm9tcHRBUElGb3JFeHRlbnNpb24iLCJleHBpcnkiOjE3NjA0ODYzOTl9"
  ],

    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+B",
            "mac": "Command+B"
          }
        }
      },
      "background": {
        "service_worker": "scripts/service-worker.js",
        "type": "module"
      },
      "minimum_chrome_version": "102",
      "omnibox": {
        "keyword": "api"
      }
}